alert('密码正确，正在跳转~');   
window.location.href="usercentre.html";
break;   
}    
testV+=1;   
var pass1 =    
prompt('密码错误','');   
}   
if (pass1!="password" & testV ==3)  
alert('密码错误，请刷新再来');  
return " ";   
}    
document.write(password());   
</script>
